	<parameter name="teacher_id" class="java.lang.Long"/>
	<parameter name="class_id" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[select t.name as name_teacher, s.registration_number,
       s.name, cl.id as class_id,
       exists (select 1 from call ca where ca.status = 2 and ca.class_id = cl.id and ca.student_id = s.id) as fj
from student s
         inner join student_class sc on (sc.student_id = s.id)
         inner join class cl on (cl.id = sc.class_id)
         inner join teacher t on (t.id = cl.teacher_id)
         inner join call c on s.id = c.student_id and cl.id = c.class_id
where cl.teacher_id = $P{teacher_id} 
and cl.id = $P{class_id} 
and status in (1,2)
and c.date = current_date
group by t.name, s.id, s.name, cl.id
order by s.name]]>
	</queryString>
